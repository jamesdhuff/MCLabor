      <Fields>
        <Field Name="workSiteName">
          <DataField>workSiteName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="workSiteRefCode">
          <DataField>workSiteRefCode</DataField>
